@model gis_final.Models.User

@{
    ViewData["Title"] = "Profile";
}

<h1>Profile</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FirstName)
        </dd>
        <dt class = "col-sm-2">
            Family
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LastName)
        </dd>
        <dt class = "col-sm-2">
            Id
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserNumber)
        </dd>
        <dt class = "col-sm-2">
            IdentityNumber
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdentityNumber)
        </dd>
        <dt class = "col-sm-2">
            Phone Number 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            Status 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserStatus)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-controller="Users" asp-route-id="@Model.Id">Edit</a>
</div>
